{
    "id": "cinema/ui",
    "container": {
      "type": "DOCKER",
      "docker": {
          "image": "stockallj/cinema-ui:201703080222",
          "network": "BRIDGE",
          "portMappings": [
            { "containerPort": 80, "hostPort": 80, "protocol": "tcp" }
          ]
      }
    },
    "acceptedResourceRoles": ["slave_public"],
    "instances": 1,
    "cpus": 0.25,
    "mem": 512,
    "disk": 0,
    "env": { "DOMAIN_SUFFIX": ".marathon.l4lb.thisdcos.directory" }
}
